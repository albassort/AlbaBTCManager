{
  "env_vars": {
    "WALADR": "bcrt1q5kterpy9ljv82csvzazrly6xn9eelv757d5n2p",
    "RPCP": "password1",
    "RPCU": "user1",
    "RPCX": "/mnt/btc/bitcoin-28.0/bin/bitcoin-cli",
    "RPCPORT": "18011"
  },
  "build_commands": {
    "1": {
      "name": "Compie and run upper",
      "shell_cmd": "nim c upper",
      "cwd": "./src/",
      "autoopen_ongoing": true,
      "autoopen": true,
      "print_result": false
    },
    "2": {
      "name": "Generate RPC",
      "shell_cmd": "$RPCX -rpcport=$RPCPORT -rpcpassword=$RPCP -rpcuser=$RPCU -regtest generatetoaddress 50 $WALADR",
      "cwd": "./src/",
      "autoopen": false,
      "print_result": true
    },
    "3": {
      "name": "Generate RPC",
      "shell_cmd": "nim c -r $ARG1",
      "cwd": "./src/",
      "autoopen": true,
      "autoopen_ongoing": true,
      "min_args": 1
    }
  }
}
